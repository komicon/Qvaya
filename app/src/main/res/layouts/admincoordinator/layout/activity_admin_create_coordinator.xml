<?xml version="1.0" encoding="utf-8"?>

<android.support.v4.widget.NestedScrollView

    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.example.thando.qvaya.AdminCoordinator.AdminCreateCoordinator"
        tools:layout_editor_absoluteY="25dp">


        <android.support.design.widget.TextInputLayout
            android:id="@+id/textInputLayout11"
            android:layout_width="0dp"

            android:layout_height="0dp"
            android:layout_marginBottom="9dp"
            android:layout_marginEnd="51dp"
            android:layout_marginLeft="51dp"
            android:layout_marginRight="51dp"
            android:layout_marginStart="51dp"
            android:layout_marginTop="16dp"
            android:hint="name"
            app:layout_constraintBottom_toTopOf="@+id/textInputLayout12"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints">

            <EditText
                android:id="@+id/EmpNameTxt"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:inputType="text"
                android:labelFor="@+id/EmpNameTxt"
                android:maxLength="100" />

        </android.support.design.widget.TextInputLayout>

        <android.support.design.widget.TextInputLayout
            android:id="@+id/textInputLayout12"

            android:layout_width="0dp"


            android:layout_height="0dp"
            android:layout_marginBottom="7dp"
            android:layout_marginEnd="51dp"
            android:layout_marginLeft="51dp"
            android:layout_marginRight="51dp"
            android:layout_marginStart="51dp"
            android:hint="surname"
            app:layout_constraintBottom_toTopOf="@+id/spinnerGender"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textInputLayout11"
            tools:ignore="MissingConstraints">

            <EditText
                android:id="@+id/EmpSurnametxt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:filterTouchesWhenObscured="true"
                android:inputType="text|textPersonName"
                android:labelFor="@+id/EmpSurnametxt"
                android:maxLength="100" />


        </android.support.design.widget.TextInputLayout>


        <android.support.design.widget.TextInputLayout
            android:id="@+id/textInputLayout22"
            android:layout_width="0dp"


            android:layout_height="0dp"
            android:layout_marginBottom="1dp"
            android:layout_marginEnd="6dp"
            android:layout_marginLeft="49dp"
            android:layout_marginRight="6dp"
            android:layout_marginStart="49dp"
            android:hint="year_started"
            app:layout_constraintBottom_toTopOf="@+id/CreateDriverAdmin"
            app:layout_constraintEnd_toStartOf="@+id/fab"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textInputLayout14"
            tools:ignore="MissingConstraints">

            <EditText
                android:id="@+id/testtxt"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:inputType="number"
                android:labelFor="@+id/testtxt"
                android:maxLength="4" />

        </android.support.design.widget.TextInputLayout>

        <android.support.design.widget.TextInputLayout
            android:id="@+id/textInputLayout15"
            android:layout_width="0dp"


            android:layout_height="0dp"
            android:layout_marginBottom="1dp"
            android:layout_marginEnd="51dp"
            android:layout_marginLeft="51dp"
            android:layout_marginRight="51dp"
            android:layout_marginStart="51dp"
            android:hint="email"
            app:layout_constraintBottom_toTopOf="@+id/textInputLayout17"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textInputLayout16"
            tools:ignore="MissingConstraints">

            <EditText
                android:id="@+id/EmailEMployeeBus"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:inputType="textEmailAddress"
                android:labelFor="@+id/EmailEMployeeBus"
                android:maxLength="100" />

        </android.support.design.widget.TextInputLayout>

        <android.support.design.widget.TextInputLayout
            android:id="@+id/textInputLayout16"
            android:layout_width="0dp"


            android:layout_height="0dp"
            android:layout_marginBottom="13dp"
            android:layout_marginEnd="51dp"
            android:layout_marginLeft="51dp"
            android:layout_marginRight="51dp"
            android:layout_marginStart="51dp"
            android:hint="age"
            app:layout_constraintBottom_toTopOf="@+id/textInputLayout15"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/spinnerGender"
            tools:ignore="MissingConstraints">

            <EditText
                android:id="@+id/AgeEmptxt"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:inputType="number"
                android:maxLength="2" />

        </android.support.design.widget.TextInputLayout>

        <android.support.design.widget.TextInputLayout
            android:id="@+id/textInputLayout17"
            android:layout_width="0dp"


            android:layout_height="0dp"
            android:layout_marginBottom="8dp"
            android:layout_marginEnd="51dp"
            android:layout_marginLeft="51dp"
            android:layout_marginRight="51dp"
            android:layout_marginStart="51dp"
            android:hint="home_address"
            app:layout_constraintBottom_toTopOf="@+id/textInputLayout14"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textInputLayout15"
            tools:ignore="MissingConstraints">

            <EditText
                android:id="@+id/HomeAddEmptxt"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:inputType="text|textAutoComplete|textMultiLine|textEmailAddress"
                android:labelFor="@+id/HomeAddEmptxt"
                android:maxLength="100" />

        </android.support.design.widget.TextInputLayout>


        <android.support.design.widget.TextInputLayout
            android:id="@+id/textInputLayout14"
            android:layout_width="0dp"


            android:layout_height="0dp"
            android:layout_marginBottom="7dp"
            android:hint="cell_number"
            app:layout_constraintBottom_toTopOf="@+id/textInputLayout22"
            app:layout_constraintEnd_toEndOf="@+id/textInputLayout22"
            app:layout_constraintStart_toStartOf="@+id/textInputLayout22"
            app:layout_constraintTop_toBottomOf="@+id/textInputLayout17"
            tools:ignore="MissingConstraints">

            <EditText
                android:id="@+id/CellNumbertxt"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:inputType="phone"
                android:maxLength="10" />

        </android.support.design.widget.TextInputLayout>


        <android.support.v7.widget.AppCompatButton

            android:id="@+id/CreateDriverAdmin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="3dp"
            android:onClick="CreateCoordinaAdminBTN"
            android:text="create_coordinator"
            android:textColor="@color/white"
            app:backgroundTint="@color/darkgrey"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textInputLayout22"
            tools:ignore="MissingConstraints" />


        <android.support.design.widget.FloatingActionButton
            android:id="@+id/fab"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="17dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:clickable="true"
            android:focusable="true"
            app:backgroundTint="@color/cardview_light_background"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textInputLayout22"
            app:srcCompat="?android:attr/homeAsUpIndicator"
            tools:ignore="MissingConstraints" />

        <Spinner
            android:id="@+id/spinnerGender"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginBottom="10dp"
            android:layout_marginEnd="53dp"
            android:layout_marginLeft="53dp"
            android:layout_marginRight="53dp"
            android:layout_marginStart="53dp"
            android:entries="@array/Gender"
            app:layout_constraintBottom_toTopOf="@+id/textInputLayout16"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textInputLayout12" />


    </android.support.constraint.ConstraintLayout>

</android.support.v4.widget.NestedScrollView>
